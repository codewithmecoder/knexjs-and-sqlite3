<table id="myTable">
  <tr class="header">
    <th>ID</th>
    <th>NAME</th>
    <th>EMAIL</th>
  </tr>
  <% user.forEach(user => { %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
    </tr>
  <% }) %>
</table>